type UserProfile = 
 record {
   createdAt: int;
   lastActive: int;
   learningPoints: nat;
   submittedClaims: vec ClaimId;
   username: opt text;
 };
type Result_1 = 
 variant {
   err: text;
   ok;
 };
type Result = 
 variant {
   err: text;
   ok: UserProfile;
 };
type ClaimId = text;
service : {
  addLearningPoints: (points: nat) -> (Result_1);
  addSubmittedClaim: (claimId: ClaimId) -> (Result_1);
  createProfile: (username: opt text) -> (Result_1);
  getProfile: () -> (Result) query;
}
